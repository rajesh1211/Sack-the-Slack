{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/sts-38.png",
    "128": "images/sts-38.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts.babel/chromereload.js",
      "scripts.babel/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "debugger",
    "storage"
  ],
  // "options_ui": {
  //   "page": "options.html",
  //   "chrome_style": true
  // },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts.babel/jquery-3.2.1.min.js",
        "scripts.babel/contentscript.js"
      ],
      "css": ["styles/content.css"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "page_action": {
    // "default_icon": {
    //   "19": "images/icon-19.png",
    //   "38": "images/icon-38.png"
    // },
    "default_title": "sack the slack",
    "default_popup": "popup.html",
    "icons": {
      "16": "images/sts-38.png",
      "128": "images/sts-38.png"
    }
  }
  
}
